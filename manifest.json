{
  "manifest_version": 2,
  "name": "BigURL Pro",
  "version": "1.0.3",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "16": "icon16.png"
  },
  "description": "Display an enlarged URL at the top of any webpage.",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["activeTab"],
  "browser_action": {},
  "web_accessible_resources": ["styles.css"]
}
